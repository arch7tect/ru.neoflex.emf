@GenModel(
    bundleManifest="false",
    modelDirectory="src/java/java-gen",
    complianceLevel="7.0",
    rootExtendsClass="org.eclipse.emf.ecore.impl.MinimalEObjectImpl",
    rootExtendsInterface="org.eclipse.emf.ecore.EObject"
)
package ru.neoflex.emf.bazi.calendar

import ru.neoflex.emf.bazi.natalChart.Pillar

annotation "ru.neoflex.emf.bazi" as BaZi

@BaZi(documentation="Солнечный календарь. Пекинское время. Года генерируются встроенной функцией один раз")
class Calendar {
    contains Year[] year
    op String createYears(String yearFrom, String yearTo)
}

@BaZi(documentation="Год начинается с февраля")
class Year {
    Integer name
    contains Pillar year
    contains Month[] months
}

class Month {
    Months name
    contains Pillar month
    contains Day[] days
}

class Day {
    Integer name
    contains Pillar day
    contains Hour[] hours
}

class Hour {
    Date timeFrom
    Date timeTo
    contains Pillar day
}

enum Months {
    January
    February
    March
    April
    May
    June
    July
    August
    September
    October
    November
    December
}